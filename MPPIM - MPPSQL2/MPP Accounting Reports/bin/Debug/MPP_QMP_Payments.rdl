  <DataSets>
    <DataSet Name="MPPIM">
      <Query>
        <DataSourceName>MPPIM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AgedDate">
            <Value>=Parameters!AgedDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select *,CAST(@AgedDate - InvoiceDate AS INT) AS DaysAged from pur.fnt_AP_Aging(@AgedDate,'d')
where
((LocationID = 1 and vendorid = '000177')
or
(LocationID = 7 and vendorid = '000295'))
and 
 (CAST(@AgedDate- InvoiceDate AS INT) &gt;= 42)</CommandText>
      </Query>
