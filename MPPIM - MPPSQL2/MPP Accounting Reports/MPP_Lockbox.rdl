  <DataSources>
    <DataSource Name="RIMWIN">
      <DataSourceReference>RIMWIN</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>be3c16f8-e5ba-4467-aac6-96697a80a931</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Report">
      <Query>
        <DataSourceName>RIMWIN</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DepositDate">
            <Value>=Parameters!DepositDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--Detail
select lb.*,  loc.LocationName, lc.LocationCheckAmount, rc.ReportCheckAmount 
from rev.BnkLockboxReceipts lb
join adm.Locations loc on lb.LocationId = loc.LocationId 
join (-- Location level
	select LocationId, sum(CheckAmount) LocationCheckAmount
	from 
		(select distinct LocationId, CheckNumber, CheckAmount
		from rev.BnkLockboxReceipts 
		where DepositDate = @DepositDate) t
	group by LocationId ) lc on lb.LocationId = lc.LocationId 
cross join
	-- Report level
	(select sum(CheckAmount) ReportCheckAmount
	from 
		(select distinct LocationId, CheckNumber, CheckAmount
		from rev.BnkLockboxReceipts 
		where DepositDate = @DepositDate) t) rc
where lb.DepositDate = @DepositDate</CommandText>
      </Query>
