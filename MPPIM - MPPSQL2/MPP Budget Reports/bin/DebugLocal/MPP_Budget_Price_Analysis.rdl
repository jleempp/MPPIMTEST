      <Query>
        <DataSourceName>RIM</DataSourceName>
        <CommandText>SELECT     salPlanBudget.LocationId, sysLocations.LocationName, salPlanBudget.CustomerId, salPlanBudget.Company, salPlanBudget.StockCode, 
                      salPlanBudget.StockCodeName, salPlanBudget.BudgetPriceY0, salPlanBudget.BudgetPriceY1, salPlanBudget.LastSellPrice
FROM         salPlanBudget INNER JOIN
                      sysLocations ON salPlanBudget.LocationId = sysLocations.LocationId
WHERE     (salPlanBudget.LocationId IN (@Location))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Location">
