      <Query>
        <DataSourceName>RIM</DataSourceName>
        <CommandText>SELECT     sysLocations.LocationName, salPlanBudget.LocationId, salPlanBudget.CustomerId, salPlanBudget.Company, salPlanBudget.StockCode, 
                      salPlanBudget.StockCodeName, salPlanBudget.BudgetPriceY0 * conYear0.Rate AS BudgetPriceY0, 
                      salPlanBudget.BudgetPriceY1 * conYear1.Rate AS BudgetPriceY1, 
                      salPlanBudget.Q0P1 + salPlanBudget.Q0P2 + salPlanBudget.Q0P3 + salPlanBudget.Q0P4 + salPlanBudget.Q0P5 + salPlanBudget.Q0P6 + salPlanBudget.Q0P7
                       + salPlanBudget.Q0P8 + salPlanBudget.Q0P9 + salPlanBudget.Q0P10 + salPlanBudget.Q0P11 + salPlanBudget.Q0P12 AS BudgetYearQuantity, 
                      (conYear0.Rate * salPlanBudget.BudgetPriceY0) 
                      * (salPlanBudget.Q0P1 + salPlanBudget.Q0P2 + salPlanBudget.Q0P3 + salPlanBudget.Q0P4 + salPlanBudget.Q0P5 + salPlanBudget.Q0P6 + salPlanBudget.Q0P7
                       + salPlanBudget.Q0P8 + salPlanBudget.Q0P9 + salPlanBudget.Q0P10 + salPlanBudget.Q0P11 + salPlanBudget.Q0P12) AS BudgetYearDollars, 
                      salPlanBudget.Q1P1 + salPlanBudget.Q1P2 + salPlanBudget.Q1P3 + salPlanBudget.Q1P4 + salPlanBudget.Q1P5 + salPlanBudget.Q1P6 + salPlanBudget.Q1P7
                       + salPlanBudget.Q1P8 + salPlanBudget.Q1P9 + salPlanBudget.Q1P10 + salPlanBudget.Q1P11 + salPlanBudget.Q1P12 AS CurrentYearQuantity, 
                      conYear1.Rate * (salPlanBudget.D1P1 + salPlanBudget.D1P2 + salPlanBudget.D1P3 + salPlanBudget.D1P4 + salPlanBudget.D1P5 + salPlanBudget.D1P6
                       + salPlanBudget.D1P7 + salPlanBudget.D1P8 + salPlanBudget.D1P9 + salPlanBudget.D1P10 + salPlanBudget.D1P11 + salPlanBudget.D1P12) 
                      AS CurrentYearDollars, 
                      salPlanBudget.Q2P1 + salPlanBudget.Q2P2 + salPlanBudget.Q2P3 + salPlanBudget.Q2P4 + salPlanBudget.Q2P5 + salPlanBudget.Q2P6 + salPlanBudget.Q2P7
                       + salPlanBudget.Q2P8 + salPlanBudget.Q2P9 + salPlanBudget.Q2P10 + salPlanBudget.Q2P11 + salPlanBudget.Q2P12 AS PriorYearQuantity, 
                      conYear2.Rate * (salPlanBudget.D2P1 + salPlanBudget.D2P2 + salPlanBudget.D2P3 + salPlanBudget.D2P4 + salPlanBudget.D2P5 + salPlanBudget.D2P6
                       + salPlanBudget.D2P7 + salPlanBudget.D2P8 + salPlanBudget.D2P9 + salPlanBudget.D2P10 + salPlanBudget.D2P11 + salPlanBudget.D2P12) 
                      AS PriorYearDollars, salPlanBudget.Comment, salPlanBudget.PlanYear, salPlanBudget.ManualAdd, salPlanBudget.CurrencyId, 
                      conYear0.Rate AS Rate0, conYear1.Rate AS Rate1, conYear2.Rate AS Rate2
FROM         sysLocations INNER JOIN
                      salPlanBudget ON sysLocations.LocationId = salPlanBudget.LocationId INNER JOIN
                      dbo.fntSalPlanBudgetCurCon(@ToCurrency, @Year - 0) AS conYear0 ON salPlanBudget.CurrencyId = conYear0.CurrencyId INNER JOIN
                      dbo.fntSalPlanBudgetCurCon(@ToCurrency, @Year - 1) AS conYear1 ON salPlanBudget.CurrencyId = conYear1.CurrencyId INNER JOIN
                      dbo.fntSalPlanBudgetCurCon(@ToCurrency, @Year - 2) AS conYear2 ON salPlanBudget.CurrencyId = conYear2.CurrencyId
WHERE     (salPlanBudget.PlanYear = @Year) AND (salPlanBudget.LocationId IN (@Location))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ToCurrency">
            <Value>=Parameters!ToCurrency.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Location">
            <Value>=Parameters!Location.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
