      <Query>
        <DataSourceName>RIM</DataSourceName>
        <CommandText>SELECT        LocationName, LocationID, CustomerId, Company, StockCodeName, BudgetYearQuantity, BudgetYearDollars, CurrentYearQuantity, CurrentYearDollars, 
                         PriorYearQuantity, PriorYearDollars, Comment, PlanYear, BudgetPriceY0, BudgetPriceY1, PricingChange, GainLoss, ManualAdd, CurrencyId, Rate0, Rate1, 
                         Rate2, CurrentAvgPrice, Other, StockCode, RowId, Source
FROM            dbo.fntRptMPPSalesBudgetRecon(@ToCurrency, @Year) AS fntRptMPPSalesBudgetRecon_1
WHERE        (LocationID IN (@Location))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ToCurrency">
            <Value>=Parameters!ToCurrency.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Location">
            <Value>=Parameters!Location.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
