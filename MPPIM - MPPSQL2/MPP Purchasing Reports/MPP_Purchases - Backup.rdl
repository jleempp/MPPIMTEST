      <Query>
        <DataSourceName>RIM</DataSourceName>
        <CommandText>SELECT        purVendors.LocationId, sysLocations.LocationName, purVendors.VendorId, purVendors.MasterVendorId, purVendors.Company, purInvoiceHeader.Invoice, 
                         purInvoiceHeader.AmountDue, purInvoiceHeader.InvoiceDate, purInvoiceHeader.Status, purInvoiceHeader.StatusId, purInvoiceHeader.TermsId, 
                         tblTerms.DaysGrace, tblTerms.DaysDue, tblTerms.Description AS TermsDesc, purMasterVendors.MasterVendorName
FROM            purInvoiceHeader INNER JOIN
                         purVendors ON purInvoiceHeader.LocationId = purVendors.LocationId AND purInvoiceHeader.VendorId = purVendors.VendorId INNER JOIN
                         sysLocations ON purVendors.LocationId = sysLocations.LocationId AND purInvoiceHeader.LocationId = sysLocations.LocationId INNER JOIN
                         tblTerms ON purInvoiceHeader.TermsId = tblTerms.TermsId INNER JOIN
                         purMasterVendors ON purVendors.MasterVendorId = purMasterVendors.MasterVendorId
WHERE        (tblTerms.Class = 'P') AND (purInvoiceHeader.InvoiceDate BETWEEN @BeginDate AND @EndDate) AND (purVendors.MasterVendorId IN (@MasterVendor)) 
                         AND (purVendors.LocationId IN (@Location))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@BeginDate">
            <Value>=Parameters!BeginDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MasterVendor">
            <Value>=Parameters!MasterVendor.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Location">
            <Value>=Parameters!Location.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
